            <% for (var i=1; i<q.q_choices.length; i++) { %>
              <% if (stu_answers[thisAssignment.a_name+'_'+q.q_id]!=null && stu_answers[thisAssignment.a_name+'_'+q.q_id].indexOf(eval('\''+i+'\''))>=0) { %>
                &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
