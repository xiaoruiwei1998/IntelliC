              <% } else { %>
                &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                <input type="checkbox" name="stu_answer<%=i%>" value="<%=i%>"> &nbsp<%=q.q_choices[i]%><br>
              <% } %>
            <% } %>
            <div class = "submit_position"><input id = "submit_button" type="submit" name="submit" value="提交"></div>
          </form>
        <% } %>
      </div>
    
      <br>
      <% }) %>
    </div>
    <% }%>





